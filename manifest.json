{
    "author": "sasidharan_gs",
    "content_scripts": [
        {
            "js": [ "res_num.js" ],
            "css": [ "2col_sidepane.css" ],
            "matches": [ "http://www.google.com/*", "https://www.google.com/*"],
            "exclude_globs": [ "*://www.google.com/*&udm=2*" ],
            "run_at": "document_end"
        }
    ],
    "description": "Improve your productivity by adding 2-column view, Result-link numbering, and Auto-hiding information side-panel to your Google results",
    "icons": {
        "128": "2g_logo.png",
        "16": "2g_logo.png",
        "48": "2g_logo.png"
    },
    "manifest_version": 3,
    "name": "Google Results Enhancer",
    "version": "1.0"
}